!include ../galera_3nodes_as_slave.cnf

[mysqld]
gtid-mode=ON
log-bin=mysqld-bin
log-slave-updates
enforce-gtid-consistency
binlog-format=ROW
wsrep_mode = IGNORE_NATIVE_REPLICATION_FILTER_RULES

[mysqld.2]
replicate-do-db=test1
replicate-wild-do-table=test1.%

[mysqld.3]
replicate-do-db=test1
replicate-wild-do-table=test1.%
